<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <localRepository/>
  <interactiveMode/>
  <usePluginRegistry/>
  <offline/>
  <pluginGroups/>
  <servers>
    <server>
      <id>nevermined-repository</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
    <server>
      <id>nevermined-programs</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
    <server>
      <id>nevermined-spigot-repo</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
    <server>
      <id>nevermined-dependencies</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
    <server>
      <id>nevermined-snapshot-repository</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
    <server>
      <id>nevermined-plugin-repository</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
    <server>
      <id>nevermined-release-repository</id>
      <username>${env.NM_NEXUS_USER}</username>
      <password>${env.NM_NEXUS_PASS}</password>
    </server>
  </servers>
  <mirrors/>
  <proxies/>
  <profiles/>
  <activeProfiles/>
</settings>